  <div
    class="icon"
    style="background-image:url('data:image/svg+xml;base64,')"
    onclick="open('/apps')">
  </div>
  <div
    class="icon"
    style="background-image:url('data:image/svg+xml;base64,')"
    onclick="open('/games')">
  </div>
</div>
